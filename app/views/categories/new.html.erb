<%= render 'shared/content_title', title: 'Create a Category' %>
<%= render 'shared/error', obj: @category %>

<div class='well'>
  <%= form_for @category do |f| %>
    <div class='control-group'>
      <%= f.label :name %>
      <%= f.text_field :name %>
    </div>
    <%= f.submit "Create Catetory", class: "btn btn-primary" %>
  <% end %>
</div>